{
  "Resources": {
    "Fn::Merge": [
      {
        "Fn::Map": [
          [
            "item0",
            "item1",
            "item2",
            "item3",
            "item4",
            "item5",
            "item6",
            "item7",
            "item8",
            "item9"
          ],
          "$",
          {
            "Bucket${$}": {
              "Type": "AWS::S3::Bucket",
              "Properties": {
                "BucketName": {
                  "Fn::Sub": "bucket-${$}"
                }
              }
            }
          }
        ]
      }
    ]
  }
}